# Example environment configuration (do NOT commit secrets)
# Copy to .env and replace values as needed.

DB_USER=rugby_app
DB_PASS=__REPLACE_WITH_STRONG_SECRET__
DB_NAME=videos_db
DB_HOST=127.0.0.1
DB_PORT=5432

# Vector / embedding tuning
EMBED_DIM=384
VECTOR_OPS=vector_cosine_ops   # or vector_l2_ops / vector_ip_ops
IVFFLAT_LISTS=100
IVFFLAT_REBUILD=0              # set to 1 to force rebuild if params differ
IVFFLAT_CONCURRENT=0           # set to 1 for CONCURRENTLY builds (less locking)

# Logging
LOG_LEVEL=INFO
